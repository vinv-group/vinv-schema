{
  "$id": "https://schema.vinv.io/diameter_at_height/0.0.1.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "title": "Height of trunk",
  "description": "Height in Meter where diameter is zero.",
  "type": [
    "object",
    "null"
  ],
  "required": [
    "height",
    "diameter"
  ],
  "properties": {
    "height": {
      "title": "Height",
      "description": "Height of the trunk.",
      "type": "number",
      "minimum": 0,
      "maximum": 100,
      "multipleOf": 0.1,
      "examples": [
        1.3
      ],
      "vinv": {
        "$vinv": {
          "unit": "Meter",
          "unitSymbol": "m"
        }
      }
    },
    "diameter": {
      "title": "Diameter",
      "description": "Diameter at max Height.",
      "type": "integer",
      "const": 0,
      "minimum": 0,
      "maximum": 1000,
      "examples": [
        0
      ],
      "vinv": {
        "$vinv": {
          "unit": "Centimeter",
          "unitSymbol": "cm"
        }
      }
    }
  }
}