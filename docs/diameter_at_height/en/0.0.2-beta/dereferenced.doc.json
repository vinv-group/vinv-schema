{
  "$id": "https://schema.vinv.io/diameter_at_height/en/0.0.2-beta.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "title": "Diameter at height",
  "description": "Array including height and diameter at height",
  "type": [
    "object",
    "null"
  ],
  "minItems": 2,
  "maxItems": 2,
  "additionalItems": false,
  "required": [
    "height",
    "diameter"
  ],
  "properties": {
    "$schema": {
      "type": "string",
      "format": "uri",
      "examples": [
        "https://schema.vinv.io/diameter_at_height/en/0.0.1.json"
      ]
    },
    "height": {
      "title": "Height",
      "description": "Height of measurment in METER where diameter is measured.",
      "type": "number",
      "minimum": 0,
      "maximum": 100,
      "multipleOf": 0.1,
      "examples": [
        0.5,
        0.1
      ],
      "$vinv": {
        "unit": "Meter",
        "unitSymbol": "m"
      }
    },
    "diameter": {
      "title": "Diameter",
      "description": "Diameter in CENTIMETER at measured height.",
      "type": "integer",
      "minimum": 1,
      "maximum": 5000,
      "examples": [
        304,
        308
      ],
      "$vinv": {
        "unit": "Centimeter",
        "unitSymbol": "cm"
      }
    }
  }
}