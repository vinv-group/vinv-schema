{
    "$id": "https://schema.vinv.io/diameter_at_height/0.0.1.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "additionalProperties": false,
    "title": "Diameter at breast height",
    "description": "Including height and diameter at breast height",
    "type": "object",
    "required": [
        "height",
        "diameter"
    ],
    "properties": {
        "height": {
            "title": "Height",
            "description": "Height of measurment in meter where diameter is measured (1.3m).",
            "type": "number",
            "const": 1.3,
            "minimum": 0,
            "maximum": 100,
            "multipleOf": 0.1,
            "examples": [
                1.3
            ],
            "vinv": {
                "vinv:unit": "Meter",
                "vinv:unitSymbol": "m"

            }
        },
        "diameter": {
            "title": "Diameter",
            "description": "Diameter in centimeter at measured height (1.3m).",
            "type": "integer",
            "minimum": 1,
            "maximum": 1000,
            "examples": [
                34,
                38
            ],
            "vinv": {
                "vinv:unit": "Centimeter",
                "vinv:unitSymbol": "cm"
            }
        }
    }
}